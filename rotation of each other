
#include <stdio.h>
#include<string.h>
void main()
{
   char a[100],b[100];
   int i,j,count=0 ;
   void *flag;
   printf("enter the 1st string\n");
   scanf("%s",a);
   printf("enter the 1st string\n");
   scanf("%s",b);
    
   if(strlen(a)==strlen(b))
   {
       strcat(a,a);
       
       flag=strstr(a,b);
       if(flag!=NULL)
       {
           printf("rotation of each other\n");
           
       }
       else
       {
           printf("not rotation\n");
       }
    
    
   }
   else
   {
       printf("not a rotation of each other");
   }
  
}
