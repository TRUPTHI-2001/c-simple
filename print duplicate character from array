#include <stdio.h>
#include<string.h>
int main()
{

    char a[100];
    int max=0,i,j,count=0,k;
    char maxchar;
    int maxcount=0;
    printf("enter the word\n");
    scanf("%s",a);
    for(i=0;i<strlen(a);i++)
    {
       
        for(j=i+1;j<strlen(a);j++)
        {
            if(a[i]==a[j])
            {
                printf("%c",a[j]);
                
            }
        }
        
    }
   
